<?xml version="1.0" ?>
<odoo>
    <record id="view_wizard_lock_report_form" model="ir.ui.view">
        <field name="name">wizard.lock.report.form.view</field>
        <field name="model">wizard.lock.report</field>
        <field name="arch" type="xml">
            <form string="Lock report">
                <group>
                    <field name="lock_date" class="oe_inline" />
                </group>
                <footer>
                    <button
                        name="action_lock_report"
                        string="Lock"
                        type="object"
                        class="oe_highlight"
                    />
                    <button string="Cancel" special="cancel" class="btn-secondary" />
                </footer>
            </form>
        </field>
    </record>
</odoo>

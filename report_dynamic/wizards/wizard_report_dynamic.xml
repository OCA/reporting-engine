<?xml version="1.0" ?>
<odoo>
    <record id="view_wizard__report_form" model="ir.ui.view">
        <field name="name">wizard.report.dynamicform.view</field>
        <field name="model">wizard.report.dynamic</field>
        <field name="arch" type="xml">
            <form string="Template">
                <group>
                    <field name="template_id" options="{'no_create': True}" />
                    <field name="model_name" />
                </group>
                <footer>
                    <button
                        name="action_generate_reports"
                        string="Generate reports"
                        type="object"
                        class="oe_highlight"
                    />
                    <button string="Cancel" special="cancel" class="btn-secondary" />
                </footer>
            </form>
        </field>
    </record>
</odoo>

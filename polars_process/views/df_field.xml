<odoo>
    <record id="df_field_tree" model="ir.ui.view">
        <field name="model">df.field</field>
        <field name="arch" type="xml">
            <list editable="top">
                <field name="sequence" widget="handle" />
                <field
                    name="dataframe_id"
                    context="{'default_dataframe_id': parent}"
                    optional="hide"
                />
                <field name="field_id" context="{'technical_name': 1}" />
                <field name="name" />
                <field name="renamed" />
                <field name="required" />
                <field name="check_type" />
            </list>
        </field>
    </record>

    <record id="action_df_field" model="ir.actions.act_window">
        <field name="name">Df Field</field>
        <field name="res_model">df.field</field>
        <field name="view_mode">list</field>
        <field name="path">df-field</field>
    </record>
</odoo>

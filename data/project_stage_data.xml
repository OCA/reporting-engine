<?xml version="1.0"?>
<odoo noupdate="0">
    <data>
        <record id="project_stage_quotation" model="project.stage">
            <field name="name">Quotation</field>
            <field name="sequence">10</field>
            <field name="first_stage" eval="True"/>
            <field name="allow_cancel" eval="True"/>
            <field name="allow_modified" eval="True"/>
            <field name="allow_deleted" eval="True"/>
            <field name="execute_function">fnc_exe_quotation</field>
            <field name="validate_function">fnc_validate_quotation</field>
            <field name="project_group_ids"
            eval="[(4, ref('default_project_group_normal'))]"/>
        </record>
        <record id="project_stage_design" model="project.stage">
            <field name="name">Design</field>
            <field name="sequence">20</field>
            <field name="first_stage" eval="False"/>
            <field name="allow_cancel" eval="False"/>
            <field name="allow_modified" eval="False"/>
            <field name="allow_deleted" eval="False"/>
            <field name="execute_function">fnc_exe_design</field>
            <field name="validate_function">fnc_validate_design</field>
            <field name="project_group_ids"
            eval="[(4, ref('default_project_group_normal'))]"/>
        </record>
        <record id="project_stage_construction" model="project.stage">
            <field name="name">Construction</field>
            <field name="sequence">30</field>
            <field name="first_stage" eval="False"/>
            <field name="allow_cancel" eval="False"/>
            <field name="allow_modified" eval="False"/>
            <field name="allow_deleted" eval="False"/>
            <field name="execute_function">fnc_exe_construction</field>
            <field name="validate_function">fnc_validate_construction</field>
            <field name="project_group_ids"
            eval="[(4, ref('default_project_group_normal'))]"/>
        </record>
        <record id="project_stage_qa" model="project.stage">
            <field name="name">QA</field>
            <field name="sequence">40</field>
            <field name="first_stage" eval="False"/>
            <field name="allow_cancel" eval="False"/>
            <field name="allow_modified" eval="False"/>
            <field name="allow_deleted" eval="False"/>
            <field name="execute_function">fnc_exe_qa</field>
            <field name="validate_function">fnc_validate_qa</field>
            <field name="project_group_ids"
            eval="[(4, ref('default_project_group_normal'))]"/>
        </record>
        <record id="project_stage_done" model="project.stage">
            <field name="name">Done</field>
            <field name="sequence">50</field>
            <field name="last_stage" eval="True"/>
            <field name="case_default" eval="True"/>
            <field name="fold" eval="True"/>
            <field name="project_group_ids"
            eval="[(4, ref('default_project_group_normal'))]"/>
        </record>
        <record id="project_stage_cancel" model="project.stage">
            <field name="name">Canceled</field>
            <field name="sequence">60</field>
            <field name="last_stage" eval="True"/>
            <field name="cancel_stage" eval="True"/>
            <field name="case_default" eval="True"/>
            <field name="fold" eval="True"/>
            <field name="project_group_ids"
            eval="[(4, ref('default_project_group_normal'))]"/>
        </record>
    </data>
</odoo>